## Program to sort an array using bubble sort
int c1() {
    int arr[6] = {5, 2, 9, 1, 5, 6};
    int n = 6;
    ## Bubble sort algorithm
    for int i = 0; i < n - 1; ++i: {
        for int j = 0; j < n - i - 1; ++j: {
            if arr[j] > arr[j + 1]: {
                ## Swap arr[j] and arr[j+1]
                int temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }

    cout << "Sorted array: ";
    for int k = 0; k < n; ++k: {
        cout << arr[k] << " ";
    }
    cout << endl;

    return 0;
}
